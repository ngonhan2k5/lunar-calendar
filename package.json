{
  "name": "zero",
  "version": "1.0.0",
  "description": "Learn react from zero",
  "main": "index.js",
  "scripts": {
    "start1": "webpack-dev-server --mode development --open --hot",
    "build": "webpack --mode production && workbox generate:sw",
    "buildAll": "webpack --mode production && webpack --mode production --config webpack.server.config.js && workbox generate:sw",
    "buildDev": "rm -rf dist && webpack --mode development --env.mode development --config webpack.server.config.js",
    "buildDev1": "rm -rf dist && webpack --mode development --env.mode development --config webpack.server.config.js && webpack --mode development --config webpack.config.js",
    "buildProd": "rm -rf dist && webpack --mode production --config webpack.server.config.js && webpack --mode production --config webpack.prod.config.js",
    "start": "node ./dist/server.js",
    "start:dev": "webpack-dev-server --mode development --hot"
  },
  "author": "ngonhan2k5",
  "license": "ISC",
  "dependencies": {
    "axios-cache-adapter": "^2.3.3",
    "babel-polyfill": "^6.26.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "memcached": "^2.2.2",
    "mongodb": "^3.2.7",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-oauth2": "^1.1.2",
    "react-popup": "^0.10.0",
    "react-router-dom": "^5.2.0",
    "webpack": "^4.32.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.23.1",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2",
    "workbox-cli": "^2.1.2"
  }
